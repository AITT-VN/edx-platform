{
  "plugins": [
    "transform-object-assign",
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "IE >= 11"
          ]
        },
        "modules": false
      }
    ],
    "babel-preset-react",
    "babel-preset-env"
  ],
  "env": {
    "test": {
      "plugins":[
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ],
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ]
      ]
    }
  }
}
